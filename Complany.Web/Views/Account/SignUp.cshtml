
@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_Auth.cshtml";
}

<h1>SignUp</h1>

<form id="registrationForm" action="">
    <div class="form-group">
        <label for="email">
            Email
        </label>
        <input type="email"
               class="form-control"
               id="email"
               placeholder="Email" required />
    </div>
    <div class="form-group">
        <label for="password">
            Password
        </label>
        <input type="password"
               class="form-control"
               id="password"
               placeholder="Password"
               required />
    </div>
    <button class="btn btn-danger">
        Login
    </button>
</form>


